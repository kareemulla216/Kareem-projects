   "source": [
    "# Generating Triangle Shape\n",
    "def triangleShape(n):\n",
    "    for i in range(n):\n",
    "        for j in range(n-i):\n",
    "            print(' ', end=' ')\n",
    "        for k in range(2*i+1):\n",
    "            print('*',end=' ')\n",
    "        print()\n",
    "row = int(input('Enter number of rows: '))\n",
    "\n",
    "triangleShape(row)"
   ]
